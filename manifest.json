{
  "manifest_version": 2,

  "name": "what To Bibtex",
  "description": "This extension converts Websites and Content to BibTex format",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
	"default_title": "Make me TeX!"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
